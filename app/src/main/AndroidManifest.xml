<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.kernlll.com.demo">

    <uses-permission android:name="android.permission.ACCEPT_HANDOVER" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />

        <service
            android:name=".service.DemoService"
            android:enabled="true"
            android:exported="true" />

        <receiver android:name=".receiver.DemoReceiver">
            <intent-filter>
                <action android:name="demo.receiver" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".volley.VolleyActivity"
            android:label="@string/title_activity_volley"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>